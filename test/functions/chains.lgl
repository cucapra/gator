float f() {
    return 1.;
}

float g(float x) {
    return x + f();
}

vec2 h(vec2 x, float y, float z) {
    return x / y * g(z);
}

void main() {
    float s1 = g(f());
    vec2 s2 = h([1., 2.], g(3.), s1);
    float s3 = g(g(g(g(g(g(3.))))));
    vec2 s4 = h(h(h([1., 2.], g(g(f())), 2.), 2., g(g(f()))), g(2.), f());
    float s5 = g(f()) + g(1.) * g(f());
    vec2 s6 = h([1., 2.] + h([2., 1.], 2., 3.), 2., 3.) * g(f());

    print s1;
    print s2;
    print s3;
    print s4;
    print s5;
    print s6;
}