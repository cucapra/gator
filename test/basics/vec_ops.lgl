type vec2 is float[2];
type vec3 is float[3];
declare float dot<t:vec2>(t x, t y);
declare t normalize<t:vec2>(t x);
declare t -<t>(t f);
declare t +<t:vec2>(t v1, t v2);
declare t *<t:vec3,u:float>(t v, u f);
declare t *<t:vec3,u:float>(u f, t v);
declare t .*<t:vec2>(t v1, t v2);

void main() {
    vec2 r1 = [1., 2.] + [-2.,-3.];
    print r1;

    vec2 r2 = [2.1,3.] + [-1.,1.];
    print r2;

    vec3 r3 = [3.,-4.,5.3] * 2.;
    print r3;

    vec3 r4 = 2. * [3.,4.2,-5.];
    print r4;

    vec2 r5 = [1.,2.4] .* [5.,0.2];
    print r5;

    float r6 = dot ([1.,2.], [3.,2.]);
    print r6;

    vec2 r7 = normalize ([2.,2.]);
    print r7;

    vec2 r8 = normalize ([2.,2.] + [1.,3.]);
    print r8;

}

