declare bool >(int i1, int i2);
declare bool <(int i1, int i2);
declare bool !(bool b);

void main() {
    if (1 == 2) { 
        print 2.2; 
    }
    else {
        print 3.2;
    }
    bool b = false; 
    if (b) { 
        print 2.2; 
    } 
    elif (b) {
        print 2.5;
    }
    else {
        print 3.2;
    }

    float x = 1.2;
    float y = 2.1;

    if (y > x) {
        print 2;
    }

    b = true;
    if (!b) { 
        print 2.2; 
    } 
    elif (!b) {
        print 2.5;
    }
    elif (b) {
        print 2.3;
    }
    elif (!b) {
        print 2.7;
    }
}
