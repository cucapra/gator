tag coord a is vec2;
tag coord b is vec2;
tag coord c is vec2;

// Gives 'in' the power to convert from anything to anything else in the same dimension
canon `u any<`t : vec2, `u : vec2>(`t v) {
    return v as! `u;
}

void main() {    
    a avec = [1., 2.];
    b bvec = [2., 1.];
    c cvec = [3., 0.];

    print avec in b;
    print avec in c;
    print cvec in b;
    print bvec in a;
}