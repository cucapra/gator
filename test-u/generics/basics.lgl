// Can be parameterized to literally anything!
`t id<`t>(`t x) {
    return x;
}

`t id_decl<`t>(`t x) {
    `t y = x;
    return y;
}

`t fst<`t, `u>(`t x, `u y) {
    `t z = x;
    return z;
}

